<div class="col-md-8 col-md-offset-2">
	<h1 style="margin-top:50px;">All Tags</h1>

	<ul id="tag" style="margin-top: 45px; margin-bottom: 50px;">
		<% @tags.each do |tag| %>
			<li>
				<h3><%= link_to tag.name, tag_path(tag) %> </h3>
			</li>
		<% end %>
	</ul>
	<hr>
	<%= link_to "<< Back to Articles List", articles_path%>

</div>